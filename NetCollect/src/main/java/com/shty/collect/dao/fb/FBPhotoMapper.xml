<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shty.collect.dao.fb.FBPhotoMapper">
	<insert id = "insert_FBPhoto" parameterType= "com.shty.collect.domain.fb.FBPhoto" useGeneratedKeys="true" keyProperty="id">
		insert into tb_facebook_photo (imgCode,imgUrl,FBTarget_id) values (#{imgCode},#{imgUrl},#{FBTarget_id})
	</insert>
</mapper>